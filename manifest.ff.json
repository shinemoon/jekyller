{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "short_name": "GMDWriter",
  "description": "__MSG_extDes__",
  "default_locale":"en",
  "version": "5.0.1",
  "browser_specific_settings": {
    "gecko": {
      "id": "{jeykller@mooninsky}",
      "strict_min_version": "109.0"
    }
  },
  "action": {
    "default_icon": {
      "16": "assets/icon_512.png",
      "128": "assets/icon_512.png"
    },
    "default_title": "Github Markdown Writer "
  },
  "icons": {
    "16": "assets/icon_512.png",
    "128": "assets/icon_512.png"
  },
  "background": {
    "scripts": ["service_worker.js"]
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://github.com/"
  ]
}